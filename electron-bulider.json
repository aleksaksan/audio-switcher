{
  "appId": "com.aleksan.audio-switcher",
  "files": ["dist", "dist-react"],
  "extraResources": [
    "dist-electron/preload.cjs",
    {
      "from": "build/icon.ico",
      "to": "dist/assets/icon.ico"
    },
    {
      "from": "build/icon.png",
      "to": "dist/assets/icon.png"
    }
  ],
  "Linux": {
    "target": ["AppImage"],
    "icon": "build/icon.png",
    "category": "Utility"
  },
  "win": {
    "target": ["portable", "msi"],
    "icon": "build/icon.ico"
  }
}